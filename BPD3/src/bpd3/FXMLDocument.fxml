<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bpd3.FXMLDocumentController">
    <children>
        <Button fx:id="button" layoutX="128.0" layoutY="136.0" onAction="#handleButtonAction" text="Login" />
      <Label layoutX="59.0" layoutY="14.0" text="Welcome to Bike Parts Distributorship!" />
      <TextField fx:id="usernameField" layoutX="77.0" layoutY="55.0" onAction="#handleButtonAction" promptText="Username" />
      <PasswordField fx:id="passwordField" layoutX="77.0" layoutY="88.0" onAction="#handleButtonAction" promptText="Password" />
      <Label fx:id="errorMsg" layoutX="64.0" layoutY="169.0" prefHeight="17.0" prefWidth="196.0" textFill="#fc0000" />
    </children>
</AnchorPane>
